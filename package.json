{
	"name": "chneau.github.io",
	"type": "module",
	"scripts": {
		"start": "rsbuild dev",
		"build": "bun run _genIcs.ts && rsbuild build && cp README.md dist/",
		"deploy": "bun run build && gh-pages --dist dist --branch master; rm -rf dist",
		"upgrade": "bun update --latest",
		"check": "deno fmt --use-tabs --quiet; oxlint --fix-dangerously --quiet; timeout 3s biome check --write --unsafe .",
		"lint": "tsc --noEmit",
		"all": "bun run upgrade; bun run check; bun run lint"
	},
	"dependencies": {
		"@ant-design/charts": "^2.6.7",
		"antd": "^6.1.3",
		"canvas-confetti": "^1.9.4",
		"dayjs": "^1.11.19",
		"es-toolkit": "^1.43.0",
		"fuse.js": "^7.1.0",
		"html2canvas": "^1.4.1",
		"i18next": "^25.7.3",
		"i18next-browser-languagedetector": "^8.2.0",
		"posthog-js": "^1.313.0",
		"react": "^19.2.3",
		"react-dom": "^19.2.3",
		"react-i18next": "^16.5.1",
		"styled-components": "^6.1.19",
		"valtio": "^2.3.0",
		"zod": "^4.3.5"
	},
	"devDependencies": {
		"@rsbuild/core": "^1.7.1",
		"@rsbuild/plugin-react": "^1.4.2",
		"@types/bun": "^1.3.5",
		"@types/canvas-confetti": "^1.9.0",
		"@types/react": "^19.2.7",
		"@types/react-dom": "^19.2.3",
		"gh-pages": "^6.3.0",
		"typescript": "^5.9.3"
	}
}
